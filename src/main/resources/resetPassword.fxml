<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controles.ResetPassword"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label layoutX="76.0" layoutY="35.0" text="Reset Password" />
        <Label layoutX="26.0" layoutY="81.0" text="New Password:" />
        <TextField fx:id="newPasswordField" layoutX="116.0" layoutY="77.0" />
        <Button fx:id="submitButton" layoutX="166.0" layoutY="128.0" mnemonicParsing="false" onAction="#handleSubmit" text="Submit" />
    </children>

</AnchorPane>
